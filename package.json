    "@storybook/addon-links": "^8.4.2",
    "@storybook/blocks": "^8.4.2",
    "@storybook/svelte": "^8.4.2",
    "@storybook/sveltekit": "^8.4.2",
    "@storybook/test": "^8.4.2",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/kit": "^2.8.3",
    "@sveltejs/vite-plugin-svelte": "^4.0.1",
