    "@storybook/blocks": "^8.4.2",
    "@storybook/svelte": "^8.4.6",
    "@storybook/sveltekit": "^8.4.6",
    "@storybook/test": "^8.4.6",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/kit": "^2.9.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.2",
    "@typescript-eslint/parser": "^8.13.0",
